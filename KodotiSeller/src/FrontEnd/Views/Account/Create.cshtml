@model bool
@{
    ViewData["Title"] = "Nuevo usuario";
}

<h1 class="title">@ViewData["Title"]</h1>
<h2 class="subtitle">Creación de usuarios</h2>

<div class="content">
    @if (Model)
    {
        <p>El usuario ha sido creado con éxito.</p>
    }
    else
    {
        <p>El usuario no ha sido creado con éxito.</p>
    }
</div>